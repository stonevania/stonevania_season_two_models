{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:using_item",
    "on_true": {
      "type": "minecraft:range_dispatch",
      "property": "minecraft:custom_model_data",
      "entries": [
        {
          "threshold": 302,
          "model": {
            "type": "minecraft:model",
            "model": "custom:item/athi/pirate_spyglass_in_hand"
          }
        },
        {
          "threshold": 303,
          "model": {
            "type": "minecraft:model",
            "model": "custom:item/athi/spyglass_in_hand"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/spyglass_in_hand"
      }
    },
    "on_false": {
      "type": "minecraft:range_dispatch",
      "property": "minecraft:custom_model_data",
      "entries": [
        {
          "threshold": 302,
          "model": {
            "type": "minecraft:model",
            "model": "custom:item/athi/pirate_spyglass"
          }
        },
        {
          "threshold": 303,
          "model": {
            "type": "minecraft:model",
            "model": "custom:item/athi/spyglass"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/spyglass"
      }
    }
  }
}
